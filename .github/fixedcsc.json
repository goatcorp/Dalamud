{
    "problemMatcher": [
        {
            "owner":"fixedcsc",
            "pattern": [
                {
                    "regexp":"^([^\\s].*)\\((\\d+),(\\d+|\\d+,\\d+)?\\):\\s+(error|warning)\\s+([a-zA-Z]+(?<!MSB)\\d+):\\s*(.*?)\\s+\\[(.*?)\\]$",
                    "file":1,
                    "line":2,
                    "column":3,
                    "severity":4,
                    "code":5,
                    "message":6,
                    "fromFile":7
                }
            ]
        }
    ]
}